<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<!-- list all the packages being supplied by this jar -->
	<RDF:Seq about="urn:mozilla:package:root">
		<RDF:li resource="urn:mozilla:package:observeclipboard"/>
	</RDF:Seq>

	<!-- package information -->
	<RDF:Description about="urn:mozilla:package:observeclipboard"
		chrome:name="observeclipboard"
		chrome:displayName="Clipboard Observer"
		chrome:author="SHIMODA Hiroshi"
		chrome:authorURL="http://piro.sakura.ne.jp/"
		chrome:settingsURL="chrome://observeclipboard/content/pref/prefDialog.xul"
		chrome:extension="true"
		chrome:description="Opens new tab or window automatically when you copy URLs into the clipboard."/>


	<!-- overlay information -->
	<RDF:Seq about="urn:mozilla:overlays">
		<RDF:li resource="chrome://navigator/content/navigatorOverlay.xul"/>
		<RDF:li resource="chrome://communicator/content/pref/preftree.xul"/>

		<RDF:li resource="chrome://browser/content/browser.xul"/>
		<RDF:li resource="chrome://global/content/customizeToolbar.xul"/>
	</RDF:Seq>


	<RDF:Seq about="chrome://navigator/content/navigatorOverlay.xul">
		<RDF:li>chrome://observeclipboard/content/observeclipboard.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://communicator/content/pref/preftree.xul">
		<RDF:li>chrome://observeclipboard/content/pref/preftreeOverlay.xul</RDF:li>
	</RDF:Seq>

	<RDF:Seq about="chrome://browser/content/browser.xul">
		<RDF:li>chrome://observeclipboard/content/observeclipboard.xul</RDF:li>
		<RDF:li>chrome://observeclipboard/content/buttonsOverlay.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://global/content/customizeToolbar.xul">
		<RDF:li>chrome://observeclipboard/content/buttonsOverlay.xul</RDF:li>
	</RDF:Seq>

</RDF:RDF>
