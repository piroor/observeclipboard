<?xml version="1.0" encoding="UTF-8"?>
<RDF:RDF xmlns:em="http://www.mozilla.org/2004/em-rdf#"
         xmlns:NC="http://home.netscape.com/NC-rdf#"
         xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <RDF:Description RDF:about="urn:mozilla:install-manifest"
                   em:id="{84BE9FF4-6D4F-4477-8E8A-86CF17F053BA}"
                   em:name="Clipboard Observer"
                   em:version="1.2.2006100801"
                   em:creator="SHIMODA Hiroshi"
                   em:description="Opens new tab or window automatically when you copy URLs into the clipboard."
                   em:homepageURL="http://piro.sakura.ne.jp/xul/_observeclipboard.html.en"
                   em:iconURL="chrome://observeclipboard/content/icon.png"
                   em:optionsURL="chrome://observeclipboard/content/pref/prefDialog.xul">


    <em:updateURL>https://pub.cozmixng.org/~piro/xul/xul.rdf.en</em:updateURL>


    <em:targetApplication>
      <!-- Firefox -->
      <RDF:Description em:id="{ec8030f7-c20a-464f-9b0e-13a3a9e97384}"
                       em:minVersion="1.0"
                       em:maxVersion="2.0.0.*" />
      <!-- Mozilla Suite -->
      <RDF:Description em:id="{86c18b42-e466-45a9-ae7a-9b95ba6f5640}"
                       em:minVersion="1.0"
                       em:maxVersion="1.8.*" />
      <!-- Netscape Browser 8 -->
      <RDF:Description em:id="{86c18b42-e466-45a9-ae7a-9b95ba6f5640}"
                       em:minVersion="8.0"
                       em:maxVersion="8.*" />
    </em:targetApplication>
	<em:file>
		<RDF:Description RDF:about="urn:mozilla:extension:file:observeclipboard.jar">
			<em:package>content/observeclipboard/</em:package>
			<em:locale>locale/en-US/observeclipboard/</em:locale>
			<em:locale>locale/ja-JP/observeclipboard/</em:locale>
			<em:skin>skin/classic/observeclipboard/</em:skin>
		</RDF:Description>
	</em:file>
  </RDF:Description>
</RDF:RDF>
